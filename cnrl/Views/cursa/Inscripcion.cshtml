                                    @if (ofertaAgrupadaEmpresa != null)
                                    {
                                        foreach (var grupo in ofertaAgrupadaEmpresa)
                                        {
                                            if (hayAlgunoInscripto) //esto es para ver si esta inscripto en alguna sede la muestra sin colapsar, si no esta inscripto a nada muestra unlam por defecto
                                            {
                                                colapasado = " ";
                                            }
                                            <div class="panel box box-success box-solid">
                                                <div class="box-header with-border ">
                                                    <h4 class="box-title ">
                                                        <a data-toggle="collapse" data-parent="#accordion2" href="#@grupo.Key.ToString().Replace(' ', '_')">
                                                            Sede: @grupo.Key
                                                            <div class="box-tools pull-right">
                                                                <button class="btn btn-box-tool" data-widget="collapse"><i class="glyphicon glyphicon-chevron-down"></i></button>
                                                            </div>
                                                        </a>

                                                    </h4>
                                                </div>
                                                <div id="@grupo.Key.ToString().Replace(' ', '_')" class="panel-collapse collapse">

                                                    @using (Html.BeginForm("Inscripcion", "Cursa", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                                                    {
                                                        <input type="hidden" name="usuario" value="@ViewBag.Usuario" />
                                                        <input type="hidden" name="callBackView" value="FormaDePago" />
                                                        <div class="box-body">
                                                            <div class="table-responsive">
                                                                <table class="table table-striped">
                                                                    <tr>
                                                                        <th style="width:35px">Tipo</th>
                                                                        <th style="width:25px">Código</th>
                                                                        <th>Nombre del Curso</th>
                                                                        <th>Día y Horario</th>
                                                                    </tr>
                                                                    @{
                                                                        IEnumerable<IGrouping<int, ofertaDisponiblePorAlumno4_Result>> cursoAgrupado = grupo.GroupBy(cur => cur.codCurso.Value);
                                                                    }

                                                                    @foreach (var cur in cursoAgrupado)
