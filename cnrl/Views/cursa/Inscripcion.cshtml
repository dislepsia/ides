                        bootbox.confirm("Se eliminarán solo las inscripciones en las que no haya seleccionado una forma de Pago, si ya seleccionó una forma de pago debe hacer una solicitud de baja de cursadas", function (result) { window.location.href = '@Url.Action("EliminarInscripcionCompleta", "Cursa", new { id = ViewBag.Usuario })'; });
                    }

                </script>

            </div>
