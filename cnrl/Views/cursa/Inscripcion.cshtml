@model IEnumerable<cnrl.ofertaDisponiblePorAlumno4_Result>
@using cnrl.Logica;

@{
    ViewBag.Title = "Index";
    var usuarioCompleto = (AspNetUsers)ViewBag.UsuarioCompleto;

    IEnumerable<IGrouping<string, ofertaDisponiblePorAlumno4_Result>> ofertaAgrupadaSocio = null;
    IEnumerable<IGrouping<string, ofertaDisponiblePorAlumno4_Result>> ofertaAgrupadaEmpresa = null;
    IEnumerable<IGrouping<string, ofertaDisponiblePorAlumno4_Result>> ofertaAgrupadaGraduado = null;

    if (Model != null && Model.Count() > 0)
    {
        ofertaAgrupadaSocio = Model.Where(x => x.sector == (int)Sectores.Idiomas).GroupBy(ofer => ofer.sedeNombre);
        ofertaAgrupadaGraduado = Model.Where(x => x.sector == (int)Sectores.Graduados).GroupBy(ofer => ofer.sedeNombre);
        ofertaAgrupadaEmpresa = Model.Where(x => x.sector == (int)Sectores.Empresas).GroupBy(ofer => ofer.sedeNombre);
        
    }

    string colapasado = "in";
    bool hayAlgunoInscripto = false;
    if (Model != null && Model.Where(x => x.selected == "selected").Count() > 0)
    {
        hayAlgunoInscripto = true;
    }
}
<style>
    .class-with-width {
        width: 80%;
    }

    .class-paddint-top-100 {
        margin-top: 100px;
    }
</style>
<h2>Inscripción de  <b>@usuarioCompleto.Apellido, @usuarioCompleto.Nombre (@usuarioCompleto.NroDocumento)</b> </h2>
