                                                                            <td>@cur.First().tipoCursoNombre</td>
                                                                            <td>@cur.Key</td>
                                                                            <td>@cur.First().descripcion</td>
                                                                            <td>
                                                                                <div class="form-group " style="margin: 0">
                                                                                    @{
                                                                                        string disabled = "";
                                                                                        if (cur.Where(x => x.selected.Equals("selected")).Count() > 0)
                                                                                        {
                                                                                            disabled = "disabled";
                                                                                        }
                                                                                    }
                                                                                    <select class="form-control" name="codigo" @disabled>
                                                                                        <option value="-1">(Seleccione un Horario)</option>
                                                                                        @foreach (var horario in cur)
                                                                                        {
                                                                                            <option value="@horario.codigo" @horario.selected>@horario.Horario</option>
                                                                                        }
                                                                                    </select>
                                                                                </div>
                                                                            </td>
                                                                        </tr>
                                                                                        }
                                                                </table>
                                                            </div>
                                                            @{ colapasado = " ";}
                                                            <input type="submit" value="Inscribirse" class="btn btn-success btn-lg pull-right" />

                                                        </div>
                                                                }
                                                </div>
                                            </div>

                                                                    }
                                                                }
                                                                else
                                                                {
                                                                    <div class="alert-dismissible alert-link">No hay cursos disponible para inscribirse</div>
                                                                }
                                </div>
                            </div>
                        </div>
                    </div>



                </div>



