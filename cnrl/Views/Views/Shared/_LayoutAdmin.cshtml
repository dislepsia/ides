    <script>
        function AjaxOk(data) {
            console.log(data);
            if (data.ok === true) {
                MensajeExito('Operación finalizada con éxito');
                $('.modal').modal('hide');
            } else {
                //Recarga el formulario con los errores recibidos
                MensajeError('Operación finalizada con errores');
                $('#DlgGenericoBody').html(data);
            }
        }
        function AjaxOk2(data) {
            console.log(data);
            if (data.ok === true) {
                MensajeExito('Operación finalizada con éxito');
                $('#DlgGenerico2').modal('hide');
                // Si existe la Grilla de Responsables, la refresca
                if (window.Responsables) {
                    window.Responsables.Refresh();
                    onGridFocusedRowChanged(window.Responsables);
                }
            } else {
                //Recarga el formulario con los errores recibidos
                MensajeError('Operación finalizada con errores');
                //$('#DlgGenericoBody2').html(data);
            }
        }
